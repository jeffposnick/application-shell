{
  "hosting": {
    "public": "www",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "/",
      "function": "index"
    }, {
      "source": "/questions/*",
      "function": "questions"
    }, {
      "source": "/about",
      "function": "about"
    }]
  }
}
